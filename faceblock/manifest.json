{
  "manifest_version": 2,
  "name": "Faceblock",
  "description": "Helps you break away from infinite scrolling sites",
  "version": "1.0",
  "browser_action": {
    "default_icon": "static/fb.png",
    "default_popup": "static/faceblock.html"
  },
  "permissions": ["webNavigation", "tabs", "https://www.facebook.com/"],
  "background": {
    "scripts": ["js/faceblock.js"],
    "persistent": false
  }
}
